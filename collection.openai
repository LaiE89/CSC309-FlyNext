  /api/visitor/flights/search:
    get:
      summary: flights-search
      description: >-
        Visitors, users, and hotel owners can call this which returns up to 5
        suggestions for an airport code or city based on the substring given by
        the query.
      operationId: flightsSearch
      parameters:
        - name: query
          in: query
          schema:
            type: string
            example: To
          description: A substring to search for an airport code or city name
      responses:
        '200':
          description: New Request
          headers:
            Connection:
              schema:
                type: string
                example: keep-alive
            Date:
              schema:
                type: string
                example: Sat, 08 Mar 2025 17:50:45 GMT
            Keep-Alive:
              schema:
                type: string
                example: timeout=5
            Transfer-Encoding:
              schema:
                type: string
                example: chunked
            vary:
              schema:
                type: string
                example: >-
                  RSC, Next-Router-State-Tree, Next-Router-Prefetch,
                  Next-Router-Segment-Prefetch
          content:
            application/json:
              schema:
                type: object
                properties:
                  suggestions:
                    type: array
                    items:
                      type: string
                      example: Tokyo
                    example:
                      - Tokyo
                      - Toronto
              examples:
                New Request:
                  value:
                    suggestions:
                      - Tokyo
                      - Toronto
